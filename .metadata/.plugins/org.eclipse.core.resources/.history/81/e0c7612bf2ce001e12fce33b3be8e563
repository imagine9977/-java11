package org.kh.java;

public class Hello {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String hello = "Hello";
		String world = "World!";
		String java1 = "Java!";
		String[] words = new String[4];
		words[0] = hello;
		words[1] = java1;
		System.out.println("Say the line Bart!");
		System.out.println(hello + " " + world);
		System.out.println(words[0]+ " "+words[1]);		
	}

}
