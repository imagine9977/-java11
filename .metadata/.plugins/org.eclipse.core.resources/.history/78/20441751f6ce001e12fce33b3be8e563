package baek;
import java.util.Scanner;
public class skipYear {
	public static void main(String[] argv) {
		Scanner sc = new Scanner(System.in);
		int ScanInt1 = sc.nextInt();
		boolean isLeap = 0;
		if(ScanInt1 %4 == 0) {
			if(ScanInt1%100 == 0) {
				if(ScanInt1%400 ==0) {
					isLeap = 1;
				}
			} else {
				isLeap =1;
			}
		}
	}
}
