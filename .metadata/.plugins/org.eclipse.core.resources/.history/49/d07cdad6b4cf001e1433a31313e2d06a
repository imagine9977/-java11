package org.kh.java;

public class Variable8 {
	public static void main(String[] args) {
		//가변 할당(동적 할당) : 자료형과 무관하게 해당하는 리터럴 (값)
		var num1 = 29.456;
		var num2 = 34;
		var num3 = 'k';
		var num4 = "Bon";
		var num5 = true;
		num1 = 67; //실수 유지
		var num6 = num1 +"";
		num1 = Integer.parseInt(num6);
		System.out.println(num1); //
		System.out.println(num6); //
		
	}
}
